#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
#welcome message
puts "Welcome to Tic Tac Toe!"
#display empty board
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
#prompt user for input
puts "Where would you like to go?"
input = gets.strip
index = input_to_index(input)
move(board, index, value = "X")
display_board(board)